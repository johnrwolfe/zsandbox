INSERT INTO S_SYNC
	VALUES ("0914f23b-5838-4f75-aaeb-e144750b76ae",
	"00000000-0000-0000-0000-000000000000",
	'getPartialPayment',
	'',
	'
//The funtion returns the payment amount based on the payment time and the ending time
// for a given payment (salary, bonus, etc.). The ending time has to occur, otherwise
// the funtion will return zero.  
if  (param.Payment_Time >= param.Ending_Time)
	days = 30 - ((param.Payment_Time - param.Ending_Time) /24*60*60);
	return days * (param.Payment/30);
else 
	return 0; 
end if;',
	"ba5eda7a-def5-0000-0000-000000000002",
	1,
	'',
	0);
